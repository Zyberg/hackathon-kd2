{
  "name": "rest-express",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "generate-client": "npx swagger-typescript-api -p ./public/swagger.json -o ../strava-frontend/src/api -n ApiClient.ts --axios",
    "dev": "concurrently --kill-others \"npx prisma generate && nodemon --exec ts-node ./src/index.ts\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "node_modules"
    ]
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "4.18.2",
    "express-jwt": "^8.4.1",
    "helmet": "^6.0.1",
    "http-status": "^1.6.2",
    "jwks-rsa": "^3.0.1",
    "nocache": "^3.0.4",
    "paginate-prisma": "^1.0.27",
    "swagger-ui-express": "^4.6.2",
    "tsoa": "^5.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "18.15.0",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.6.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "ts-node": "10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.9.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
