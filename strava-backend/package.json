{
  "name": "rest-express",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc --skipLibCheck",
    "migrate": "prisma migrate dev",
    "schema": "node ./src/helpers/validation/schemaGenerator.js",
    "generate-client": "npx swagger-typescript-api -p ./public/swagger.json -o ../strava-frontend/src/api -n ApiClient.ts --axios",
    "dev": "prisma migrate dev && concurrently --kill-others \"npx prisma generate && nodemon --exec ts-node ./src/index.ts\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "node_modules"
    ]
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@quixo3/prisma-session-store": "^3.1.10",
    "@riderize/passport-strava-oauth2": "^2.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "4.18.2",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^8.4.1",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "jwks-rsa": "^3.0.1",
    "nocache": "^3.0.4",
    "paginate-prisma": "^1.0.27",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "strava-v3": "^2.1.0",
    "swagger-ui-express": "^4.6.2",
    "tsoa": "^5.1.1"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/morgan": "^1.9.4",
    "@types/node": "18.15.0",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/riderize__passport-strava-oauth2": "^2.1.1",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.6.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "ts-node": "10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.9.5",
    "typescript-json-schema": "^0.55.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
